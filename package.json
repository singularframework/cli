{
  "name": "@singular/cli",
  "version": "1.0.0-alpha.2",
  "description": "Singular framework command line interface for project management",
  "main": "bin/sg.js",
  "bin": {
    "sg": "bin/sg.js"
  },
  "files": [
    "/bin",
    "/template"
  ],
  "scripts": {
    "start": "npm run cleanTestDist && npm run buildSrc",
    "prepublishOnly": "npm run cleanTestDist && npm run buildSrc",
    "test": "npm run cleanTestDist && npm run buildSrc && npm run cleanTestDist && npm run buildTestSrc && npm run runMocha",
    "cleanBin": "node ./node_modules/rimraf/bin.js \"./bin\"",
    "buildSrc": "node ./node_modules/typescript/bin/tsc --build",
    "cleanTestDist": "node ./node_modules/rimraf/bin.js \"./test/dist\"",
    "buildTestSrc": "node ./node_modules/typescript/bin/tsc -p ./test/tsconfig.json",
    "runMocha": "node ./node_modules/mocha/bin/mocha --config ./test/.mocharc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/singularframework/cli.git"
  },
  "keywords": [
    "singular",
    "framework",
    "nodejs",
    "node",
    "typescript",
    "backend",
    "server",
    "express",
    "http",
    "cli"
  ],
  "author": "Ramtin Soltani <ramtin@chiselpowered.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/singularframework/cli/issues"
  },
  "homepage": "https://github.com/singularframework/cli#readme",
  "dependencies": {
    "argumental": "^1.1.1",
    "chalk": "^4.1.0",
    "chokidar": "^3.5.1",
    "fs-extra": "^9.1.0",
    "glob": "^7.1.6",
    "lodash": "^4.17.20",
    "mustache": "^4.1.0",
    "ora": "^5.3.0",
    "semver": "^7.3.4",
    "source-map-support": "^0.5.19",
    "terser": "^5.5.1",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/fs-extra": "^9.0.6",
    "@types/glob": "^7.1.3",
    "@types/lodash": "^4.14.168",
    "@types/luxon": "^1.25.1",
    "@types/mocha": "^8.2.0",
    "@types/mustache": "^4.1.1",
    "@types/node": "^14.14.22",
    "@types/semver": "^7.3.4",
    "@types/source-map-support": "^0.5.3",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "got": "^11.8.1",
    "luxon": "^1.25.0",
    "mocha": "^8.2.1",
    "mocha-progress-reporter": "^1.1.3",
    "rimraf": "^3.0.2",
    "typedoc": "^0.20.19",
    "typescript": "^4.1.3"
  }
}
