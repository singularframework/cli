{
  "compilerOptions": {
    "baseUrl": "./",
    "sourceMap": false,
    "inlineSourceMap": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2018",
    "sourceRoot": ".",
    "outDir": "../dist",
    "typeRoots": [
      "../node_modules/@types"
    ],
    "lib": [
      "es2018"
    ],
    "paths": {
      "@{{{projectName}}}/services/*": ["./{{{servicePathPrefix}}}*.service"],
      "@{{{projectName}}}/config": ["./server.config.json"]
    }
  }
}
