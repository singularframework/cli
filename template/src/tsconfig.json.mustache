{
  "compilerOptions": {
    "baseUrl": "./",
    "sourceMap": false,
    "inlineSourceMap": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2018",
    "sourceRoot": ".",
    "outDir": "../dist",
    "typeRoots": [
      "../node_modules/@types"
    ],
    "lib": [
      "es2018"
    ],
    "paths": {
      "@{{{projectName}}}/service/*": ["./{{{servicePathPrefix}}}*.service"],
      "@{{{projectName}}}/router/*": ["./{{{routerPathPrefix}}}*.router"],
      "@{{{projectName}}}/interceptor/*": ["./{{{interceptorPathPrefix}}}*.interceptor"],
      "@{{{projectName}}}/plugin/*": ["./{{{pluginPathPrefix}}}*.plugin"],
      "@{{{projectName}}}/config": ["./server.config"]
    }
  }
}
